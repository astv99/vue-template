{
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "prettier"
  ],
  "rules": {
    "vue/no-v-html": 0,
    "vue/multi-word-component-names": 0
  },
  "env": {
    "vue/setup-compiler-macros": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "espree",
    "ecmaVersion": 2022,
    "sourceType": "module"
  }
}
